{
    "name": "typekeeper",
    "version": "1.1.2",
    "private": false,
    "main": "dist/typekeeper.js",
    "types": "dist/typekeeper.d.ts",
    "scripts": {
        "prebuild": "rm -rf ./dist",
        "build": "tsc --project tsconfig-build.json",
        "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
        "check-types": "tsc",
        "prettier": "prettier --ignore-path .eslintignore \"**/*.+(js|json|ts|tsx)\"",
        "format": "npm run prettier -- --write",
        "check-format": "npm run prettier -- --check",
        "validate": "npm run check-types && npm run check-format && npm run lint && npm run build",
        "validate-parallel": "npm-run-all --parallel check-types check-format lint build",
        "test": "jest",
        "test:ci": "jest --ci",
        "test-coverage": "jest --collectCoverage=true",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "test:badges": "npm run test-coverage  && jest-coverage-badges",
        "test:readme-badges": "npm run test:badges && jest-coverage-badges output badges",
        "prepublish": "rm -rf dist && tsc"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "typescript",
        "type helper",
        "type gaurds",
        "type assertion"
    ],
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "conventional-changelog-cli": "^2.2.2",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-next": "11.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "jest": "^27.2.0",
        "jest-coverage-badges": "^1.1.2",
        "lint-staged": "^11.1.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "ts-jest": "^27.1.3",
        "typescript": "4.3.5"
    },
    "lint-staged": {
        "*.+(js|ts|tsx)": [
            "eslint"
        ],
        "*.+(js|json|ts|tsx)": [
            "prettier --write",
            "git add"
        ]
    }
}
